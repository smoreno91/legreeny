                (function($) {
                    if (typeof EM == 'undefined') EM = {};
                    if (typeof EM.SETTING == 'undefined') EM.SETTING = {};
                    function getWindowWidth() {
                        var w = window;
                        var d = document;
                        var e = d.documentElement;
                        var g = d.getElementsByTagName('body')[0];
                        var x = w.innerWidth||e.clientWidth||g.clientWidth;
                    	return x;
