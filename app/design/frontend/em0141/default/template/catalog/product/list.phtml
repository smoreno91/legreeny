                            var sDesktop = 'emcatalog-desktop-';
                            var sGrid = $('#em-grid-mode');
                            sGrid.removeClass().addClass(sDesktop+'<?php echo $_columnCountDesktop ?>');
                        }
                        
                    };
                    
                    //Ready Function
                    $(document).ready(function() {
                        setColumnCountGridMode();
                    });
